{
  "name": "appsass-core",
  "version": "0.0.1",
  "description": "appsass core",
  "author": "Jam Risser",
  "keywords": [
    "nestjs",
    "prisma",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/appsass-core.git"
  },
  "bugs": {
    "url": "https://github.com/codejamninja/appsass-core/issues"
  },
  "scripts": {
    "build": "make -s build",
    "clean": "make -s clean",
    "coverage": "make -s coverage",
    "database": "make -s database",
    "docker:build": "make -s docker-build",
    "docker:pull": "make -s docker-pull",
    "docker:push": "make -s docker-push",
    "docker:ssh": "make -s docker-ssh",
    "docker:up": "make -s docker-up",
    "env": "make -s env",
    "format": "make -s format",
    "lint": "make -s lint",
    "make": "make -s",
    "prepare": "make -s prepare",
    "prisma:generate": "make -s prisma-generate",
    "prisma:generate:watch": "make -s prisma-generate-watch",
    "prisma:save": "make -s prisma-save",
    "prisma:studio": "make -s prisma-studio",
    "prisma:up": "make -s prisma-up",
    "purge": "make -s purge",
    "seed": "make -s seed",
    "spellcheck": "make -s spellcheck",
    "start": "make -s start",
    "test": "make -s test",
    "test:ui": "make -s test-ui",
    "test:watch": "make -s test-watch"
  },
  "dependencies": {
    "@nestjs/common": "7.0.13",
    "@nestjs/config": "0.4.2",
    "@nestjs/core": "7.0.13",
    "@nestjs/graphql": "7.3.9",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.0.13",
    "@nestjs/swagger": "4.5.7",
    "@prisma/client": "2.0.0-beta.5",
    "apollo-server-express": "2.13.1",
    "bcrypt": "4.0.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "graphql": "15.0.0",
    "graphql-tools": "6.0.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "swagger-ui-express": "4.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "7.1.5",
    "@nestjs/testing": "7.0.13",
    "@prisma/cli": "2.0.0-alpha.1236",
    "@types/bcrypt": "3.0.0",
    "@types/chance": "1.1.0",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.5",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "chance": "1.1.5",
    "cspell": "^4.0.63",
    "dotenv": "^8.2.0",
    "eslint": "7.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "gnumake": "^0.3.3",
    "jest": "26.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "lockfile-lint": "^4.3.6",
    "majestic": "^1.7.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "26.0.0",
    "ts-loader": "7.0.4",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "~3.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "eslintIgnore": [
    "dist"
  ],
  "jestSonar": {
    "reportPath": "node_modules/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "docker": {
    "username": "codejamninja"
  }
}
